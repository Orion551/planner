{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "src/",
    "paths": {
      "@planner/*": ["./*"],
      "@Assets/*": ["./src/Assets/*"],
      "@Components/*": ["./src/Components/*"],
      "@Pages/*": ["./src/Pages/*"],
      "@Utils/*": ["./src/Utils/*"]
    },
    "allowSyntheticDefaultImports": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
